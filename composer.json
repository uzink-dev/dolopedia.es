{
    "name": "symfony/framework-standard-edition",
    "license": "MIT",
    "type": "project",
    "description": "The \"Symfony Standard Edition\" distribution",
    "autoload": {
        "psr-0": { "": "src/" }
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "jquery/jquery",
                "version": "1.9.1",
                "dist": {
                    "url": "https://code.jquery.com/jquery-1.9.1.js",
                    "type": "file"
                }
            }
        }
    ],
    "require": {
        "php": ">=7.2.5",
        "apy/datagrid-bundle": "^3.2",
        "azine/email-bundle": "^4.0",
        "braincrafted/bootstrap-bundle": "2.2.0",
        "doctrine/data-fixtures": "1.2.2",
        "doctrine/doctrine-bundle": "~1.2",
        "doctrine/doctrine-fixtures-bundle": "2.3.0",
        "doctrine/orm": "~2.2,>=2.2.3",
        "egeloen/ckeditor-bundle": "~2.0",
        "friendsofsymfony/elastica-bundle": "^4.0.0",
        "friendsofsymfony/message-bundle": "2.*",
        "friendsofsymfony/rest-bundle": "1.8.0",
        "friendsofsymfony/user-bundle": "~2.0@dev",
        "helios-ag/fm-elfinder-bundle": "7.*",
        "hwi/oauth-bundle": "1.1.*",
        "incenteev/composer-parameter-handler": "~2.0",
        "jms/metadata": "1.6.0",
        "jms/serializer-bundle": "2.0.0",
        "jquery/jquery": "1.9.*",
        "knplabs/doctrine-behaviors": "1.6.*",
        "knplabs/knp-gaufrette-bundle": "0.4.0",
        "knplabs/knp-menu-bundle": "~2.0",
        "knplabs/knp-paginator-bundle": "2.6",
        "kriswallsmith/assetic": "1.4.0",
        "liip/imagine-bundle": "1.8.0",
        "lrotherfield/form": "1.1.2",
        "nelmio/api-doc-bundle": "2.13.2",
        "oneup/uploader-bundle": "1.7.7",
        "php-http/guzzle6-adapter": "^2.0",
        "php-http/httplug-bundle": "^1.18",
        "sensio/distribution-bundle": "5.*",
        "sensio/framework-extra-bundle": "~3.0",
        "sensio/generator-bundle": "3.1.*",
        "shtumi/useful-bundle": "dev-master",
        "stof/doctrine-extensions-bundle": "1.2.2",
        "symfony/assetic-bundle": "~2.3",
        "symfony/monolog-bundle": "~2.4",
        "symfony/security-acl": "3.*",
        "symfony/swiftmailer-bundle": "~2.3",
        "symfony/symfony": "~3.4",
        "symfony/workflow": "~3.4",
        "twbs/bootstrap": "3.0.*",
        "twig/extensions": "~1.0",
        "twitter/bootstrap": "2.3.2",
        "vich/uploader-bundle": "1.4.0",
        "white-october/pagerfanta-bundle": "1.0.8",
        "whiteoctober/breadcrumbs-bundle": "1.2.3"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ]
    },
    "config": {
        "bin-dir": "bin",
        "sort-packages": true
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": {
            "dev-master": "2.4-dev"
        }
    }
}
