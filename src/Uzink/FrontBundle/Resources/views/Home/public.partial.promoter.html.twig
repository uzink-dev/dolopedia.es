{% if promoters|length %}
<div class="row container-row section">
    <div class="col-md-12">
        <h2 class="centered title-big no-border">Colaboradores y patrocinadores</h2>
        <p class="sponsor-logos">
        {% for promoter in promoters %}
            <a href="{{ promoter.link }}"><img src="{{ promoter.image|centerHandler('promoter_thumb_home') }}"/></a>
        {% endfor %}
        </p>
    </div>
</div>
{% endif %}